{
  "trailingComma": "all",
  "arrowParens": "always",
  "singleQuote": false,
  "tabWidth": 4,
  "bracketSpacing": true,
  "semi": true,
  "importOrder": [
    "^@core/(.*)$",
    "^@server/(.*)$",
    "^@ui/(.*)$",
    "^[./]"
  ],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true
}